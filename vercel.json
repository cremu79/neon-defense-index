// vercel.json
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com **data:**;"
          // 이 줄을 아래와 같이 수정하세요:
          // value: "default-src 'self'; font-src 'self' https://fonts.gstatic.com data:;" 
          
          /* 최종 수정된 모습: */
          "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com **data:**;" 
        }
      ]
    }
  ]
}
