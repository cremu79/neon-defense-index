// vercel.json (수정할 부분)
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          // 'data:' 또는 **data:** 대신 순수한 data: 로 수정
          "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com **data:**;"
        }
      ]
    }
  ]
}
