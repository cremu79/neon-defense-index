// vercel.json
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          // 'data:' 대신 data: 만 사용하도록 수정
          "value": "default-src 'self'; font-src 'self' https://fonts.gstatic.com **data:**;"
        }
      ]
    }
  ]
}
